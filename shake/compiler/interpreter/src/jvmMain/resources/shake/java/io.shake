import java.java.io.FileInputStream;
import java.java.io.BufferedInputStream;
import java.java.io.File;
import java.java.lang.StringBuilder;

class FileSystem {
	
	public void readFileSync(String filename) {
		
		var in = new BufferedInputStream(new FileInputStream(new File(filename)));
		var sb = new StringBuilder();
		var available = in.available();
		
		for(int i = 0 ; i < available; i++) sb.append(in.read());
		
		return sb.toString();
	}
	
}

const fs = new FileSystem();